{
  "name": "eleventy-with-vite",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "//": "Development",
    "dev": "concurrently --raw npm:dev:*",
    "dev:tsc:watch": "tsc --watch --preserveWatchOutput",
    "dev:eleventy": "eleventy --serve",
    "dev:vite": "vite",
    "//": "Build for production",
    "build": "npm run build:tsc && npm run build:vite && npm run build:eleventy",
    "build:tsc": "tsc",
    "build:eleventy": "eleventy",
    "build:vite": "vite build",
    "//": "Example of the built site running",
    "prod": "NODE_ENV=production npm run build && serve _site"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/standalone": "7.13.9",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@vitejs/plugin-legacy": "^1.3.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "concurrently": "^6.0.0",
    "serve": "^11.3.2",
    "typescript": "^4.2.3",
    "vite": "^2.0.5"
  },
  "author": "",
  "license": "ISC"
}
